import java.util.Scanner;


public class Test {
	
	public static void main(String[] args) {
		
		 int x;
		 int y;
		 
		 Scanner input =new Scanner(System.in);
		 System.out.println("请输入x的值:");
		 x=input.nextInt();
		 System.out.println("请输入y的值:");
		 y=input.nextInt(); 
		 
		 System.out.println("x:"+x);
		 System.out.println("y:"+y);
		 
		
		
	}

}
