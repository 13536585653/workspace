package com.test.SetTest;

import java.util.Random;

public class Test2 {
	
	//产生10个不重复的随机数范围 1-20
	public static void main(String[] args) {
		
		 Random  random=new Random();
         int  x;
         for(int i=1;i<=10;i++){
			 
			 x=random.nextInt(20)+1;
			 
			 System.out.println(x);
		 }

		
	}
	

}
