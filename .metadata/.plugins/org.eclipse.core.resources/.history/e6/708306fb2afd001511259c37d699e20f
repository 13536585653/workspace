package com.test.testStatic;

public class Test {
	
	public static void main(String[] args) {
		
		A  a1=new A();
	    A  a2=new A();
		
		a1.x=5;
		a2.x=10;
		
		a1.X=5;
		a2.X=10;
		
		System.out.println(a1.x);
		System.out.println(a2.x);
		System.out.println("***********");
		System.out.println(a1.X);
		System.out.println(a2.X);
	   
		
		
		
	}

}
