package Customer_and_Producter;

public class test {
	public static void main(String[] args) {
		Store store=new Store();
		Customer c=new Customer(store);
		Producter p=new Producter(store);
		
		for(int i=1;i<=3;i++){
			new Thread(p,"生产者"+i).start();
		}
		
		for(int i=1;i<=2;i++){
			new Thread(c,"消费者"+i).start();
		}
	}
}
