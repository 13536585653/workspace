package com.test.ListTest;

import java.util.ArrayList;

public class Test1 {
	
	public static void main(String[] args) {
		
		ArrayList list=new ArrayList();   //创建一个列表
		
		list.add(1);
		list.add("abc");
		
		Dog dog1=new Dog("小白","哈士奇");
		list.add(dog1);
		
		System.out.println("list的大小为:"+list.size());
		
		
	}

}
